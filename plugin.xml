<?xml version='1.0' encoding='utf-8'?>
    <plugin id="com.fami2u.plugin.alipay" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
        <name>alipay</name>
        <PID>pid</PID>
		<engines>
		<engine name="cordova" version=">=3.4.0"/>
		</engines>
        <js-module name="alipay" src="www/alipay.js">
            <clobbers target="alipay" />
        </js-module>
        <platform name="ios">
            <config-file parent="/*" target="config.xml">
                <feature name="alipay">
                    <param name="ios-package" value="alipay" />
                </feature>
            </config-file>
            <source-file src="src/ios/alipay.m" />
        </platform>
        <platform name="android">
            <config-file parent="/*" target="res/xml/config.xml">
                <feature name="alipay">
                    <param name="android-package" value="com.fami2u.plugin.alipay.alipay" />
                </feature>
            </config-file>
            <config-file parent="/*" target="AndroidManifest.xml"></config-file>
            <source-file src="src/android/alipay.java" target-dir="src/com/fami2u/plugin/alipay/alipay" />
        </platform>
    </plugin>
